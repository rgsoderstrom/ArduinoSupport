<Window x:Class="FpgaTestDataGen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Plot2D_Embedded="clr-namespace:Plot2D_Embedded;assembly=Plot2D_Embedded"
        xmlns:local="clr-namespace:FpgaTestDataGen"
        mc:Ignorable="d"
        Title="MainWindow" Height="870" Width="900" Top="100">
    <DockPanel>
        <StackPanel Orientation="Vertical">
            <Plot2D_Embedded:Bare2DPlot Name="DoublePlots"  Margin="1,0,1,10" PlotWindowReady="Double_PlotWindowReady" MinHeight="400"/>
            <Plot2D_Embedded:Bare2DPlot Name="IntegerPlots" Margin="1,0,1,0" PlotWindowReady="Integer_PlotWindowReady" MinHeight="400"/>
        </StackPanel>
    </DockPanel>
</Window>
